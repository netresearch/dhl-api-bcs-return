{
    "name": "dhl/sdk-api-bcs-returns",
    "description": "DHL Paket Retoure API SDK",
    "license": "MIT",
    "keywords": [
        "php",
        "sdk",
        "DHL"
    ],
    "homepage": "https://entwickler.dhl.de/group/ep/wsapis/retouren",
    "authors": [
        {
            "name": "Christoph AÃŸmann",
            "email": "christoph.assmann@netresearch.de"
        }
    ],
    "require": {
        "php": "^7.1.0 || ^8.0",
        "ext-json": "*",
        "php-http/discovery": "^1.0.0",
        "php-http/httplug": "^2.0.0",
        "php-http/logger-plugin": "^1.0.0",
        "psr/http-client": "^1.0.0",
        "psr/http-client-implementation": "^1.0.0",
        "psr/http-factory": "^1.0.0",
        "psr/http-factory-implementation": "^1.0.0",
        "psr/http-message": "^1.0.0",
        "psr/http-message-implementation": "^1.0.0",
        "psr/log": "^1.1.0"
    },
    "require-dev": {
        "nyholm/psr7": "^1.0.0",
        "phpunit/phpunit": ">= 7.0 < 9.0",
        "rkr/php-http-mock-client-psr-provider": "^0.1.0",
        "phpstan/phpstan": ">= 0.1",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Dhl\\Sdk\\Paket\\Retoure\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dhl\\Sdk\\Paket\\Retoure\\Test\\": "test/"
        }
    },
    "scripts": {
        "phpunit": "phpunit -c test/phpunit.xml",
        "phpstan": "phpstan analyze -c phpstan.neon --memory-limit=2G"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "2.0.x-dev"
        }
    }
}
